{
  "name": "@glade/demos",
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev:webgl:native": "bun ./demo-webgl.ts",
    "dev:webgl:web": "bun build ./demo-webgl.ts --outdir=./dist --minify && bun ./serve.ts",
    "dev:webgpu:native": "bun ./demo-webgpu.ts",
    "dev:webgpu:web": "bun build ./demo-webgpu.ts --outdir=./dist --minify && bun ./serve.ts",
    "build:webgl:native": "bun build ./demo-webgl.ts --compile --outfile=./dist/demo-webgl",
    "build:webgl:web": "bun build ./demo-webgl.ts --outdir=./dist --minify",
    "build:webgpu:native": "bun build ./demo-webgpu.ts --compile --outfile=./dist/demo-webgpu",
    "build:webgpu:web": "bun build ./demo-webgpu.ts --outdir=./dist --minify",
    "run:webgl:native": "bun build ./demo-webgl.ts --compile --outfile=./dist/demo-webgl && ./dist/demo-webgl",
    "run:webgl:web": "bun build ./demo-webgl.ts --outdir=./dist --minify && bun ./serve.ts & sleep 1 && open 'http://localhost:3000/index.html'",
    "run:webgpu:native": "bun build ./demo-webgpu.ts --compile --outfile=./dist/demo-webgpu && ./dist/demo-webgpu",
    "run:webgpu:web": "bun build ./demo-webgpu.ts --outdir=./dist --minify && bun ./serve.ts & sleep 1 && open 'http://localhost:3000/index.html'",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@glade/core": "workspace:*",
    "@glade/platform": "workspace:*"
  }
}
